{
  "name": "Nothing OS Dark (Glyph)",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "foreground": "#FFFFFF",
    "focusBorder": "#E8E8E8",
    "editor.background": "#000000",
    "editor.foreground": "#E8E8E8",
    "editorLineNumber.foreground": "#333333",
    "editorLineNumber.activeForeground": "#E8E8E8",
    "editorCursor.foreground": "#E8E8E8",
    "editor.selectionBackground": "#FFFFFF20",
    "editor.lineHighlightBackground": "#0A0A0A",
    
    "activityBar.background": "#000000",
    "activityBar.foreground": "#E8E8E8",
    "activityBar.activeBorder": "#E8E8E8",
    "activityBarBadge.background": "#E8E8E8",
    "activityBarBadge.foreground": "#000000",
    
    "sideBar.background": "#000000",
    "sideBar.foreground": "#CCCCCC",
    
    "tab.activeBorderTop": "#E8E8E8",
    "tab.activeBackground": "#0A0A0A",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveBackground": "#000000",
    "tab.inactiveForeground": "#666666",
    
    "statusBar.background": "#000000",
    "statusBar.foreground": "#888888",
    "statusBarItem.remoteBackground": "#E8E8E8",
    "statusBarItem.remoteForeground": "#000000",
    
    "button.background": "#E8E8E8",
    "button.foreground": "#000000",
    "button.hoverBackground": "#FFFFFF",
    
    "badge.background": "#E8E8E8",
    "badge.foreground": "#000000",
    
    "terminal.ansiRed": "#D71921",
    "terminal.background": "#000000",
    "terminal.foreground": "#E8E8E8",
    "terminalCursor.foreground": "#E8E8E8",
    
    "list.highlightForeground": "#E8E8E8",
    "list.activeSelectionBackground": "#FFFFFF15",
    
    "progressBar.background": "#E8E8E8",
    
    "editorBracketHighlight.foreground1": "#E8E8E8",
    "editorBracketHighlight.foreground2": "#888888",
    "editorBracketHighlight.foreground3": "#555555"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#444444",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#E8E8E8"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#E8E8E8"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#E8E8E8"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Error Accent",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#D71921"
      }
    }
  ]
}