{
    "$schema": "vscode://schemas/color-theme",
    "name": "Nothing OS Special",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
      // Editor colors
      "editor.background": "#000000",
      "editor.foreground": "#ffffff",
      "editorLineNumber.foreground": "#6b7d85",
      "editorLineNumber.activeForeground": "#b4b4b4",
      "editor.selectionBackground": "#012f6b80",
      "editor.selectionHighlightBackground": "#505a6040",
      "editorCursor.foreground": "#ffc703",
      "editorWhitespace.foreground": "#6b7d85",
      "editorIndentGuide.background1": "#6b7d85",
      "editorIndentGuide.activeBackground1": "#b4b4b4",
      "editor.lineHighlightBackground": "#012f6b40",
      "editor.lineHighlightBorder": "#ffc703",
      
      // Editor widgets
      "editorWidget.background": "#000000",
      "editorWidget.border": "#6b7d85",
      "editorSuggestWidget.background": "#000000",
      "editorSuggestWidget.border": "#6b7d85",
      "editorSuggestWidget.foreground": "#ffffff",
      "editorSuggestWidget.highlightForeground": "#ffc703",
      "editorSuggestWidget.selectedBackground": "#c80f2e40",
      
      // Sidebar
      "sideBar.background": "#000000",
      "sideBar.foreground": "#ffffff",
      "sideBarTitle.foreground": "#ffffff",
      "sideBarSectionHeader.background": "#012f6b40",
      "sideBarSectionHeader.foreground": "#ffffff",
      "sideBarSectionHeader.border": "#6b7d85",
      
      // Activity Bar
      "activityBar.background": "#000000",
      "activityBar.foreground": "#ffffff",
      "activityBar.inactiveForeground": "#6b7d85",
      "activityBar.activeBackground": "#ffc70340",
      "activityBarBadge.background": "#ffc703",
      "activityBarBadge.foreground": "#000000",
      "activityBar.activeBorder": "#ffc703",
      
      // Status Bar
      "statusBar.background": "#000000",
      "statusBar.foreground": "#ffffff",
      "statusBar.noFolderBackground": "#000000",
      "statusBar.debuggingBackground": "#c80f2e",
      "statusBar.border": "#6b7d85",
      
      // Title Bar
      "titleBar.activeBackground": "#000000",
      "titleBar.activeForeground": "#ffffff",
      "titleBar.inactiveBackground": "#000000",
      "titleBar.inactiveForeground": "#6b7d85",
      "titleBar.border": "#6b7d85",
      
      // Tabs
      "tab.activeBackground": "#012f6b80",
      "tab.activeForeground": "#ffffff",
      "tab.inactiveBackground": "#000000",
      "tab.inactiveForeground": "#b4b4b4",
      "tab.border": "#6b7d85",
      "tab.activeBorder": "#ffc703",
      "tab.hoverBackground": "#ffc70330",
      "tab.unfocusedActiveBackground": "#000000",
      "tab.unfocusedActiveForeground": "#b4b4b4",
      
      // Panel
      "panel.background": "#000000",
      "panel.border": "#6b7d85",
      "panelTitle.activeForeground": "#ffffff",
      "panelTitle.inactiveForeground": "#b4b4b4",
      "panelInput.border": "#6b7d85",
      
      // Input
      "input.background": "#000000",
      "input.foreground": "#ffffff",
      "input.border": "#6b7d85",
      "inputOption.activeBorder": "#ffc703",
      "input.placeholderForeground": "#6b7d85",
      "inputValidation.errorBackground": "#c80f2e",
      "inputValidation.errorBorder": "#c80f2e",
      "inputValidation.infoBackground": "#012f6b",
      "inputValidation.infoBorder": "#012f6b",
      "inputValidation.warningBackground": "#ffc703",
      "inputValidation.warningBorder": "#ffc703",
      
      // Button
      "button.background": "#6b7d85",
      "button.foreground": "#ffffff",
      "button.hoverBackground": "#ffc703",
      "button.secondaryForeground": "#ffffff",
      "button.secondaryBackground": "#6b7d85",
      "button.secondaryHoverBackground": "#b4b4b4",
      
      // List
      "list.activeSelectionBackground": "#012f6b80",
      "list.activeSelectionForeground": "#ffffff",
      "list.inactiveSelectionBackground": "#012f6b40",
      "list.inactiveSelectionForeground": "#b4b4b4",
      "list.hoverBackground": "#ffc70330",
      "list.hoverForeground": "#ffffff",
      "list.focusBackground": "#012f6b80",
      "list.focusForeground": "#ffffff",
      "list.highlightForeground": "#e85f6f",
      
      // Tree
      "tree.indentGuidesStroke": "#6b7d85",
      
      // Scrollbar
      "scrollbarSlider.background": "#505a6080",
      "scrollbarSlider.hoverBackground": "#b4b4b480",
      "scrollbarSlider.activeBackground": "#c80f2e80",
      
      // Errors & Warnings
      "errorForeground": "#c80f2e",
      "editorError.foreground": "#c80f2e",
      "editorWarning.foreground": "#ffc703",
      "editorInfo.foreground": "#4a6fa5",
      "problemsErrorIcon.foreground": "#c80f2e",
      "problemsWarningIcon.foreground": "#ffc703",
      "problemsInfoIcon.foreground": "#4a6fa5",
      
      // Git
      "gitDecoration.modifiedResourceForeground": "#ffc703",
      "gitDecoration.deletedResourceForeground": "#c80f2e",
      "gitDecoration.untrackedResourceForeground": "#b4b4b4",
      "gitDecoration.ignoredResourceForeground": "#6b7d85",
      "gitDecoration.conflictingResourceForeground": "#c80f2e",
      
      // Search
      "editor.findMatchBackground": "#ffc70380",
      "editor.findMatchHighlightBackground": "#ffc70340",
      "editor.findRangeHighlightBackground": "#c80f2e40",
      "searchEditor.findMatchBackground": "#ffc70380",
      
      // Bracket Matching
      "editorBracketMatch.background": "#c80f2e40",
      "editorBracketMatch.border": "#ffc703",
      
      // Diff Editor
      "diffEditor.insertedTextBackground": "#012f6b40",
      "diffEditor.removedTextBackground": "#c80f2e40",
      "diffEditor.border": "#6b7d85",
      
      // Terminal
      "terminal.background": "#000000",
      "terminal.foreground": "#ffffff",
      "terminalCursor.foreground": "#ffc703",
      "terminal.ansiBlack": "#000000",
      "terminal.ansiRed": "#c80f2e",
      "terminal.ansiGreen": "#b4b4b4",
      "terminal.ansiYellow": "#ffc703",
      "terminal.ansiBlue": "#012f6b",
      "terminal.ansiMagenta": "#b4b4b4",
      "terminal.ansiCyan": "#dbdbdc",
      "terminal.ansiWhite": "#ffffff",
      "terminal.ansiBrightBlack": "#6b7d85",
      "terminal.ansiBrightRed": "#c80f2e",
      "terminal.ansiBrightGreen": "#b4b4b4",
      "terminal.ansiBrightYellow": "#ffc703",
      "terminal.ansiBrightBlue": "#012f6b",
      "terminal.ansiBrightMagenta": "#b4b4b4",
      "terminal.ansiBrightCyan": "#dbdbdc",
      "terminal.ansiBrightWhite": "#ffffff",
      
      // Breadcrumbs
      "breadcrumb.foreground": "#b4b4b4",
      "breadcrumb.focusForeground": "#ffffff",
      "breadcrumb.activeSelectionForeground": "#ffc703",
      "breadcrumbPicker.background": "#000000",
      
      // Dropdown
      "dropdown.background": "#000000",
      "dropdown.foreground": "#ffffff",
      "dropdown.border": "#6b7d85",
      
      // Checkbox
      "checkbox.background": "#000000",
      "checkbox.border": "#6b7d85",
      "checkbox.foreground": "#ffffff",
      
      // Progress Bar
      "progressBar.background": "#ffc703",
      
      // Notification
      "notificationCenter.border": "#6b7d85",
      "notificationCenterHeader.background": "#000000",
      "notificationToast.border": "#6b7d85",
      "notifications.background": "#000000",
      "notifications.border": "#6b7d85",
      "notificationCenterHeader.foreground": "#ffffff",
      "notifications.foreground": "#ffffff",
      "notificationLink.foreground": "#ffc703",
      
      // Badge
      "badge.background": "#ffc703",
      "badge.foreground": "#000000",
      
      // Text Block Quote
      "textBlockQuote.background": "#6b7d85",
      "textBlockQuote.border": "#6b7d85",
      
      // Text Code Block
      "textCodeBlock.background": "#000000",
      
      // Text Link
      "textLink.foreground": "#ffc703",
      "textLink.activeForeground": "#ffd84d",
      
      // Text Separator
      "textSeparator.foreground": "#6b7d85"
    },
    "tokenColors": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "foreground": "#6b7d85",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Keyword",
        "scope": [
          "keyword",
          "storage.type",
          "storage.modifier",
          "keyword.operator.new",
          "keyword.other.important"
        ],
        "settings": {
          "foreground": "#ffd84d",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Control Flow Keywords",
        "scope": [
          "keyword.control.conditional",
          "keyword.control.loop",
          "keyword.control.return",
          "keyword.control.break",
          "keyword.control.continue",
          "keyword.control.exception"
        ],
        "settings": {
          "foreground": "#d93d4f",
          "fontStyle": "bold"
        }
      },
      {
        "name": "String",
        "scope": [
          "string",
          "string.quoted",
          "string.template",
          "punctuation.definition.string"
        ],
        "settings": {
          "foreground": "#a3b4cc"
        }
      },
      {
        "name": "Number",
        "scope": [
          "constant.numeric"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "Constant",
        "scope": [
          "constant.language.boolean",
          "constant.language.null",
          "constant.language.undefined",
          "constant.other"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "Function",
        "scope": [
          "entity.name.function",
          "support.function",
          "meta.function-call.generic",
          "meta.method-call",
          "entity.name.method"
        ],
        "settings": {
          "foreground": "#5a7fb8"
        }
      },
      {
        "name": "Variable",
        "scope": [
          "variable",
          "variable.other",
          "variable.parameter",
          "meta.definition.variable"
        ],
        "settings": {
          "foreground": "#e1e6f0"
        }
      },
      {
        "name": "Type & Class",
        "scope": [
          "entity.name.type",
          "entity.name.class",
          "support.type",
          "support.class",
          "storage.type.class",
          "entity.name.namespace"
        ],
        "settings": {
          "foreground": "#6b8fc9"
        }
      },
      {
        "name": "Property",
        "scope": [
          "variable.other.property",
          "support.type.property-name"
        ],
        "settings": {
          "foreground": "#5a7fb8"
        }
      },
      {
        "name": "Special Properties",
        "scope": [
          "variable.other.readwrite",
          "variable.other.object.property"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "Tag",
        "scope": [
          "entity.name.tag",
          "entity.name.tag.html",
          "entity.name.tag.xml"
        ],
        "settings": {
          "foreground": "#6b8fc9"
        }
      },
      {
        "name": "Import/Export",
        "scope": [
          "keyword.control.import",
          "keyword.control.export",
          "keyword.control.from",
          "keyword.control.as"
        ],
        "settings": {
          "foreground": "#5a7fb8"
        }
      },
      {
        "name": "Operator",
        "scope": [
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.operator.comparison"
        ],
        "settings": {
          "foreground": "#b4b4b4"
        }
      },
      {
        "name": "Special Operators",
        "scope": [
          "keyword.operator.assignment",
          "keyword.operator.arrow",
          "keyword.operator.spread",
          "keyword.operator.rest"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "Punctuation",
        "scope": [
          "punctuation",
          "punctuation.separator",
          "punctuation.terminator",
          "punctuation.accessor"
        ],
        "settings": {
          "foreground": "#b4b4b4"
        }
      },
      {
        "name": "Decorator",
        "scope": [
          "punctuation.decorator",
          "entity.name.function.decorator"
        ],
        "settings": {
          "foreground": "#5a7fb8"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#c80f2e",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup - Heading",
        "scope": [
          "markup.heading",
          "entity.name.section"
        ],
        "settings": {
          "foreground": "#ffd84d",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup - Link",
        "scope": [
          "markup.underline.link",
          "string.other.link"
        ],
        "settings": {
          "foreground": "#c80f2e",
          "fontStyle": "underline"
        }
      },
      {
        "name": "Markup - Bold",
        "scope": [
          "markup.bold"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup - Italic",
        "scope": [
          "markup.italic"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Markup - Quote",
        "scope": [
          "markup.quote"
        ],
        "settings": {
          "foreground": "#b4b4b4",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Markup - Code",
        "scope": [
          "markup.inline.raw",
          "markup.fenced_code.block"
        ],
        "settings": {
          "foreground": "#ffd84d"
        }
      },
      {
        "name": "JSON - Key",
        "scope": [
          "support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#ffd84d"
        }
      },
      {
        "name": "JSON - String",
        "scope": [
          "string.quoted.double.json"
        ],
        "settings": {
          "foreground": "#a3b4cc"
        }
      },
      {
        "name": "JSON - Number",
        "scope": [
          "constant.numeric.json"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "JSON - Boolean/Null",
        "scope": [
          "constant.language.boolean.json",
          "constant.language.null.json"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "CSS - Property",
        "scope": [
          "support.type.property-name.css"
        ],
        "settings": {
          "foreground": "#5a7fb8"
        }
      },
      {
        "name": "CSS - Value",
        "scope": [
          "support.constant.property-value.css"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "CSS - Selector",
        "scope": [
          "entity.name.tag.css",
          "entity.other.attribute-name.class.css",
          "entity.other.attribute-name.id.css"
        ],
        "settings": {
          "foreground": "#6b8fc9"
        }
      },
      {
        "name": "HTML - Attribute",
        "scope": [
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "foreground": "#5a7fb8"
        }
      },
      {
        "name": "HTML - Attribute Value",
        "scope": [
          "string.quoted.double.html",
          "string.quoted.single.html"
        ],
        "settings": {
          "foreground": "#a3b4cc"
        }
      },
      {
        "name": "Diff - Added",
        "scope": [
          "markup.inserted.diff"
        ],
        "settings": {
          "foreground": "#b4b4b4"
        }
      },
      {
        "name": "Diff - Removed",
        "scope": [
          "markup.deleted.diff"
        ],
        "settings": {
          "foreground": "#c80f2e"
        }
      },
      {
        "name": "TypeScript/JavaScript - This",
        "scope": [
          "variable.language.this",
          "variable.language.self"
        ],
        "settings": {
          "foreground": "#ffd84d",
          "fontStyle": "italic"
        }
      },
      {
        "name": "TypeScript - Interface",
        "scope": [
          "entity.name.type.interface"
        ],
        "settings": {
          "foreground": "#6b8fc9"
        }
      },
      {
        "name": "TypeScript - Type Parameter",
        "scope": [
          "entity.name.type.parameter"
        ],
        "settings": {
          "foreground": "#6b8fc9"
        }
      },
      {
        "name": "Python - Self Parameter",
        "scope": [
          "variable.parameter.function.language.python"
        ],
        "settings": {
          "foreground": "#8a8a8a",
          "fontStyle": "italic"
        }
      },
      {
        "name": "RegExp",
        "scope": [
          "string.regexp"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "Storage Modifiers",
        "scope": [
          "storage.modifier.async",
          "storage.modifier.static",
          "storage.modifier.abstract"
        ],
        "settings": {
          "foreground": "#d93d4f"
        }
      },
      {
        "name": "Special Constants",
        "scope": [
          "constant.language.infinity",
          "constant.numeric.infinity",
          "constant.language.nan"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "Access Modifiers",
        "scope": [
          "storage.modifier.access",
          "keyword.other.access-modifier"
        ],
        "settings": {
          "foreground": "#d93d4f"
        }
      },
      {
        "name": "Template String Expression",
        "scope": [
          "meta.template.expression"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "YAML - Key",
        "scope": [
          "entity.name.tag.yaml",
          "support.type.property-name.yaml"
        ],
        "settings": {
          "foreground": "#ffd84d"
        }
      },
      {
        "name": "YAML - Keyword",
        "scope": [
          "keyword.other.definition.yaml"
        ],
        "settings": {
          "foreground": "#ffd84d"
        }
      },
      {
        "name": "YAML - String",
        "scope": [
          "string.unquoted.yaml",
          "string.quoted.double.yaml",
          "string.quoted.single.yaml"
        ],
        "settings": {
          "foreground": "#a3b4cc"
        }
      },
      {
        "name": "YAML - Number",
        "scope": [
          "constant.numeric.yaml"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      },
      {
        "name": "YAML - Boolean",
        "scope": [
          "constant.language.boolean.yaml"
        ],
        "settings": {
          "foreground": "#e85f6f"
        }
      }
    ]
  }